{
  "name": "@devtools-at/text-reverser",
  "version": "1.0.0",
  "description": "Reverse text strings",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "text-reverser",
    "devtools",
    "utility"
  ],
  "author": "DevTools.at",
  "license": "MIT",
  "homepage": "https://devtools.at/tools/text-reverser",
  "repository": {
    "type": "git",
    "url": "https://github.com/devtools-at/text-reverser.git"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}